<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/premium_fragment_ll"
    style="@style/LinearVerticalLayout"
    android:paddingLeft="15dp"
    tools:context=".view.fragment.PremiumFragment">

    <TextView
        style="@style/TextViewMatchWidthLayout"
        android:layout_marginTop="10dp"
        android:text="@string/get_premium"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <TextView
        style="@style/TextViewMatchWidthLayout"
        android:layout_marginTop="@dimen/_10dp"
        android:text="@string/premium_title"
        android:textSize="14sp" />


    <include
        android:id="@+id/service_one_product_one"
        layout="@layout/product_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp" />

    <include
        android:id="@+id/service_one_product_two"
        layout="@layout/product_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp" />

    <include
        android:id="@+id/service_one_product_three"
        layout="@layout/product_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp" />

   <!-- <Button
        android:id="@+id/btnManageSubscription"
